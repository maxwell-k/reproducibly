{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "pip_requirements": {
    "fileMatch": [
      "constraints.txt"
    ]
  },
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "^noxfile.py$"
      ],
      "matchStrings": [
        "    \"(?<depName>.+?)==(?<currentValue>.+?)\","
      ],
      "datasourceTemplate": "pypi"
    }
  ]
}
