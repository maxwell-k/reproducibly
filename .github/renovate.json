{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "assignees": [
    "maxwell-k"
  ],
  "extends": [
    "config:recommended",
    "github>kachick/renovate-config-dprint#1.3.0"
  ],
  "pip_requirements": {
    "managerFilePatterns": [
      "/constraints.txt/"
    ]
  },
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^noxfile.py$/"
      ],
      "matchStrings": [
        "    \"(?<depName>.+?)==(?<currentValue>.+?)\","
      ],
      "datasourceTemplate": "pypi"
    }
  ]
}
